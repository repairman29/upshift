{
  "buildCommand": "echo 'Static site in web/'",
  "outputDirectory": "web",
  "redirects": [
    {
      "source": "/favicon.ico",
      "destination": "/favicon.svg",
      "permanent": false
    },
    {
      "source": "/blog",
      "destination": "/blog/index.html",
      "permanent": false
    },
    {
      "source": "/blog/",
      "destination": "/blog/index.html",
      "permanent": false
    },
    {
      "source": "/radar",
      "destination": "/radar/index.html",
      "permanent": false
    },
    {
      "source": "/radar/",
      "destination": "/radar/index.html",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/start",
      "destination": "/start.html"
    },
    {
      "source": "/github-app-installed",
      "destination": "/github-app-installed.html"
    },
    {
      "source": "/radar",
      "destination": "/radar/index.html"
    },
    {
      "source": "/radar/",
      "destination": "/radar/index.html"
    },
    {
      "source": "/blog/introducing-upshift",
      "destination": "/blog/introducing-upshift.html"
    },
    {
      "source": "/blog/when-it-breaks-guardrails-hitl",
      "destination": "/blog/when-it-breaks-guardrails-hitl.html"
    },
    {
      "source": "/blog/how-we-do-hitl",
      "destination": "/blog/how-we-do-hitl.html"
    },
    {
      "source": "/blog/new-additions-and-improvements",
      "destination": "/blog/new-additions-and-improvements.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    },
    {
      "source": "/styles.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/favicon.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}